$(function(){
  <% if @report.valid? %>
  $('#flash-messages').html("<%= j render partial: 'shared/flash_messages', flash: flash %> ");
  <% flash[:notice] = nil %>
  $('#new-report-modal').modal('hide');
<% else %>
  $('#flash-messages').html("<%= j render partial: 'shared/flash_messages', flash: flash %> ");
  $('#report-error-container').html("<%= j render 'shared/errors', record: @attachment %>");
  <% flash[:error] = nil %>
<% end %>
});