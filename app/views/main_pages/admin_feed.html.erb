
  <h2>Hello, <%= current_user.username %>. Welcome to the Administrator's feed</h2>

<div class="row">
    <div class="col-md-6">
        <div class="panel with-nav-tabs panel-primary">
            <div class="panel-heading">
              <ul class="nav nav-tabs">
                  <li class="active"><a href="#reports-tab" data-toggle="tab">Reports</a></li>
                  <li><a href="#tab2primary" data-toggle="tab">Guides</a></li>
                  <li><a href="#tab3primary" data-toggle="tab">Primary 3</a></li>
              </ul>
            </div>
            <div class="panel-body">
              <div class="tab-content">
                  <div class="tab-pane fade in active" id="reports-tab">

                  <%= render 'reports', reports: @reports %>

                  </div>
                  <div class="tab-pane fade" id="tab2primary">
                  
                  <%= render 'unapproved_guides', unapproved_guides: @unapproved_guides %>

                  </div>
                  <div class="tab-pane fade" id="tab3primary">
                  
                    Misc.

                  </div>
              </div>
            </div>
        </div>
    </div>
</div>
